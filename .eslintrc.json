{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "ENV": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "no-console": ["error", { "allow": ["warn", "error"] }]
    }
}
